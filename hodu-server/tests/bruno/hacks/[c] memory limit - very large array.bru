meta {
  name: [c] memory limit - very large array
  type: http
  seq: 3
}

post {
  url: http://127.0.0.1:8080/api/v1/submit
  body: json
  auth: none
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "language": "C",
    "code": "#include <stdlib.h>\nint result[100000][100000];\nint main() { }"
  }
}

assert {
  res.status: eq 200
  res.body.stderr: eq Caught fatal signal 11\n
}
