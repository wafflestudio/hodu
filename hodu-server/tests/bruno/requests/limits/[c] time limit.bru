meta {
  name: [c] time limit
  type: http
  seq: 2
}

post {
  url: {{baseURL}}/api/v1/submit
  body: json
  auth: none
}

body:json {
  {
    "language": "c",
    "code": "#include <stdio.h>\nint main(){ int sum = 0; for (int i = 0; i < 1000000000; i++) sum += i; printf(\"%d\", sum); }",
    "stdin": "",
    "expected_stdout": "",
    "time_limit": 1,
    "fields": ["*"]
  }
}

assert {
  res.status: eq 200
  res.body.status: eq time_limit_exceeded
}
